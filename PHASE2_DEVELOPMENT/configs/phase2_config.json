{
  "phase": "PHASE2",
  "version": "2.0.0",
  "components": {
    "multimodal_inference": {
      "enabled": true,
      "vision_models": [
        "clip",
        "dinov2",
        "vit"
      ],
      "text_models": [
        "llama",
        "mistral",
        "qwen"
      ]
    },
    "advanced_serving": {
      "vllm": {
        "enabled": true,
        "max_model_len": 4096,
        "dtype": "float16",
        "gpu_memory_utilization": 0.9
      },
      "triton": {
        "enabled": true,
        "model_repository": "/models",
        "grpc_port": 8001,
        "http_port": 8000
      }
    },
    "enterprise_apis": {
      "rest": {
        "enabled": true,
        "port": 8080
      },
      "grpc": {
        "enabled": true,
        "port": 50051
      }
    }
  },
  "performance": {
    "max_batch_size": 32,
    "max_concurrent_requests": 1000,
    "request_timeout": 60.0
  },
  "monitoring": {
    "metrics_port": 9090,
    "log_level": "INFO"
  }
}